{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=ReelkixVision;Trusted_Connection=True;Encrypt=False; TrustServerCertificate=True;"
    },
    "AWS": {
        "BucketName": "reelkix-vision",
        "Region": "ap-southeast-1",
        "UploadFolder": "kix"
    },
    "FeatureFlags": {
        "DatabaseLogging": "false"
    },
    "ReelkixVisionApi": {
        "Url": "https://reelkix-vision.onrender.com/api/analyze"
    },
    "CorsSettings": {
        "AllowedOrigins": [ "https://localhost:4200", "https://reelkix-vision-web.vercel.app", "https://reelkix-vision-react.vercel.app" ]
    }
}
